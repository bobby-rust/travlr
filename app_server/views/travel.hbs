<!-- Website template by freewebsitetemplates.com -->
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="description" content="Travel Booking Website" />
        <meta name="keywords" content="travel, booking, vacation, trip" />
        <title>Dive Sites - Bhaccasyoniztas Beach Resort Website Template</title>
        <link rel="stylesheet" href="css/style.css" type="text/css" />
    </head>
    <body>
        {{> header }}
        <div id="background">
            <div id="page">
                <div id="contents">
                    <div class="box">
                        <div>
                            <div class="body">
                                <h1>Travel</h1>
                                <ul id="sites">
                                    {{#each data }}
                                        <li>
                                            <a href="/travel">
                                                <img src="images/{{ this.image }}" alt="{{ this.name }}" />
                                            </a>
                                            <h2>
                                                <a href="/travel">{{ this.name }}</a>
                                            </h2>
                                            {{{ this.description }}}
                                        </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{> footer }}
    </body>
</html>
